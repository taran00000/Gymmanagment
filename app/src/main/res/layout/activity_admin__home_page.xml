<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/linearback"
                    android:id="@+id/user_ad_list"
                    android:layout_margin="5dp"
                    android:padding="10dp"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/workout"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/User_List"
                        android:gravity="center"
                        android:fontFamily="@font/blackjack"
                        android:textStyle="bold"
                        android:textColor="#FFFFFF"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:id="@+id/trainer_ad_list"
                    android:layout_margin="5dp"
                    android:background="@drawable/gradientback"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/man"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/Trianer_List"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:fontFamily="@font/blackjack"
                        android:textColor="#222121"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_margin="5dp"
                    android:padding="10dp"
                    android:background="@drawable/gradientback"
                    android:id="@+id/admin_add_programme"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/assignment"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/Add_Programm"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:fontFamily="@font/blackjack"
                        android:textColor="#222121"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:layout_margin="5dp"
                    android:id="@+id/viewfeedback"
                    android:background="@drawable/linearback"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/feedback"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/View_Feedback"
                        android:gravity="center"
                        android:fontFamily="@font/blackjack"
                        android:textStyle="bold"
                        android:textColor="#FAF9F9"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/linearback"
                    android:id="@+id/add_trainer"
                    android:layout_margin="5dp"
                    android:padding="10dp"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/vendor"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/add_trainer"
                        android:gravity="center"
                        android:fontFamily="@font/blackjack"
                        android:textStyle="bold"
                        android:textColor="#FFFFFF"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:id="@+id/add_user"
                    android:layout_margin="5dp"
                    android:background="@drawable/gradientback"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/addbusiness"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="@string/add_user"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:fontFamily="@font/blackjack"
                        android:textColor="#222121"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_margin="5dp"
                    android:padding="10dp"
                    android:background="@drawable/gradientback"
                    android:id="@+id/admin_viewprofile"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/profilem"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="View Profile"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:fontFamily="@font/blackjack"
                        android:textColor="#222121"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:layout_margin="5dp"
                    android:id="@+id/requestlist"
                    android:background="@drawable/linearback"
                    android:layout_weight="1">
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="220dp"
                        android:padding="10dp"
                        android:src="@drawable/mail"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="22dp"
                        android:text="Request List"
                        android:gravity="center"
                        android:fontFamily="@font/blackjack"
                        android:textStyle="bold"
                        android:textColor="#FAF9F9"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>